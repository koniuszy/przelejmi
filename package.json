{
  "name": "invoice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "test": "NODE_ENV=test jest --runInBand",
    "build": "yarn db:push && yarn gen && next build",
    "gen": "yarn gen:prisma && yarn gen:nexus && yarn gen:gql",
    "gen:prisma": "prisma generate",
    "gen:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/graphql/schema",
    "gen:gql": "graphql-codegen --config codegen.yml",
    "db:push": "prisma db push --preview-feature",
    "db:deploy": "prisma migrate deploy --preview-feature",
    "db:migrate": "prisma migrate dev --preview-feature",
    "db:studio": "prisma studio",
    "serve": "NODE_ENV=production npx next start -p 4000",
    "ts-check": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@chakra-ui/icons": "^1.0.12",
    "@chakra-ui/react": "^1.6.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@plaiceholder/base64": "^1.0.0",
    "@plaiceholder/next": "^1.0.0",
    "@prisma/client": "2.21.2",
    "@types/react-paginate": "^6.2.1",
    "apollo-server-micro": "^2.23.0",
    "date-fns": "^2.21.1",
    "formik": "^2.2.6",
    "framer-motion": "^4.1.9",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.0.7",
    "graphql-shield": "^7.5.0",
    "jspdf": "^2.3.1",
    "jspdf-autotable": "^3.5.14",
    "lodash.debounce": "^4.0.8",
    "lottie-react": "^2.1.0",
    "next": "10.1.3",
    "next-auth": "^3.17.0",
    "nextjs-progressbar": "^0.0.9",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.34.1",
    "prisma": "^2.21.2",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "sharp": "^0.28.1",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/eslint-parser": "^7.13.14",
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@types/jest": "^26.0.22",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "typescript": "^4.2.4"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "builds": [
    {
      "src": "package.json",
      "config": {
        "newPipeline": true
      }
    }
  ]
}
