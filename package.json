{
  "name": "invoice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "test": "NODE_ENV=test jest --runInBand",
    "build": "yarn db:push && yarn gen && next build",
    "build:inspect": "ANALYZE=true yarn build",
    "gen": "yarn gen:prisma && yarn gen:nexus && yarn gen:gql",
    "gen:prisma": "prisma generate",
    "gen:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/graphql/schema",
    "gen:gql": "graphql-codegen --config codegen.yml",
    "db:push": "prisma db push",
    "db:deploy": "prisma migrate deploy",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "serve": "NODE_ENV=production npx next start -p 4000",
    "ts-check": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@chakra-ui/icons": "^1.0.14",
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@prisma/client": "2.27.0",
    "@types/react-paginate": "^7.1.1",
    "apollo-server-micro": "^3.0.2",
    "date-fns": "^2.23.0",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.1",
    "graphql-middleware": "^6.0.10",
    "graphql-shield": "^7.5.0",
    "jspdf": "^2.3.1",
    "jspdf-autotable": "^3.5.15",
    "lodash.debounce": "^4.0.8",
    "lottie-react": "^2.1.0",
    "micro": "^9.3.4",
    "next": "11.0.1",
    "next-auth": "^3.27.3",
    "nextjs-progressbar": "^0.0.11",
    "nexus": "^1.1.0",
    "nexus-plugin-prisma": "^0.35.0",
    "prisma": "^2.27.0",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "sharp": "^0.28.3",
    "ts-node": "^10.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@graphql-codegen/cli": "1.21.7",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@graphql-codegen/typescript-react-apollo": "2.3.1",
    "@next/bundle-analyzer": "^11.0.1",
    "@next/eslint-plugin-next": "11.0.1",
    "@types/jest": "^26.0.24",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^16.4.1",
    "@types/react": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "babel-jest": "^27.0.6",
    "eslint": "7.31.0",
    "eslint-config-alloy": "4.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "^27.0.6",
    "prettier": "2.3.2",
    "typescript": "^4.3.5"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  }
}
