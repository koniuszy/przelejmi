{
  "name": "invoice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "test": "NODE_ENV=test jest --runInBand",
    "build": "yarn db:push && yarn gen && next build",
    "gen": "yarn gen:prisma && yarn gen:nexus && yarn gen:gql",
    "gen:prisma": "prisma generate",
    "gen:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/graphql/schema",
    "gen:gql": "graphql-codegen --config codegen.yml",
    "db:push": "prisma db push",
    "db:deploy": "prisma migrate deploy",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "serve": "NODE_ENV=production npx next start -p 4000",
    "ts-check": "tsc -p tsconfig.json",
    "tidy": "eslint --fix --ext .ts,.tsx ."
  },
  "dependencies": {
    "@apollo/client": "^3.4.17",
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.1",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@prisma/client": "2.23.0",
    "@types/react-paginate": "^7.1.1",
    "apollo-server-micro": "^3.5.0",
    "date-fns": "^2.25.0",
    "formik": "^2.2.9",
    "framer-motion": "^5.3.0",
    "graphql": "^15.5.1",
    "jspdf": "^2.4.0",
    "jspdf-autotable": "^3.5.23",
    "lodash.debounce": "^4.0.8",
    "lottie-react": "^2.1.0",
    "micro": "^9.3.4",
    "next": "12.0.4",
    "next-auth": "^3.29.0",
    "nextjs-progressbar": "^0.0.13",
    "nexus": "^1.1.0",
    "nexus-plugin-prisma": "^0.35.0",
    "nexus-shield": "^2.1.0",
    "prisma": "2.23.0",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "sharp": "^0.29.3",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typescript": "2.4.0",
    "@graphql-codegen/typescript-operations": "2.2.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.1",
    "@next/eslint-plugin-next": "12.0.4",
    "@types/jest": "^27.0.2",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-jest": "^27.3.1",
    "eslint": "8.2.0",
    "eslint-config-alloy": "4.4.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "jest": "^27.3.1",
    "prettier": "2.4.1",
    "typescript": "^4.4.4"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  }
}
